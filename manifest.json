{
  "manifest_version": 2,
  "name": "Doraemon",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["storage","tabs"],
  "browser_action": {
    "default_icon": "vm.png",
    "default_popup": "popup.html"
  }
}
